@model AuthRequest

<div class="reg_content">
    <form class="reg-inner-content" asp-action="Login">
        <div class="form-group">
            <label asp-for="Email">Email</label>
            <input asp-for="Email" type="text" class="form-control" id="inputEmail">
        </div>
        <div class="form-group">
            <label asp-for="Password">Password</label>
            <input asp-for="Password" type="password" class="form-control" id="inputPassword">
        </div>
        <button type="submit" class="btn btn-primary reg-btn">Войти</button>
    </form>
</div>