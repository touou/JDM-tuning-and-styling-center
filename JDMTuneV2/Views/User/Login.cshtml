@model AuthRequest

<h3>
    Авторизация
</h3>

<div class="newslatter-form">
    <div class="row">
        <div class="col-lg-12">
            <form class="reg-form" asp-action="Login">
                <input asp-for="Email" type="text" placeholder="Почта">
                <input asp-for="Password" type="password" placeholder="Пароль">
                <button type="submit">Войти</button>
            </form>
        </div>
    </div>
</div>